{
  //---------- Cursor AI ----------//
  "cursor.cpp.disabledLanguages": ["plaintext", "markdown", "scminput"],
  "cursor.composer.shouldAutoAcceptDiffs": false,
  //---------- Custom ----------//
  "terminal.external.osxExec": "kitty.app",
  "files.autoGuessEncoding": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "search.useIgnoreFiles": false,
  //---------- Workbench ----------//
  "workbench.colorTheme": "Syntax Material Dark",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  //---------- Window ----------//
  "window.dialogStyle": "custom",
  "window.restoreWindows": "all",
  //---------- Editor ----------//
  "editor.fontFamily": "FiraMono Nerd Font Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 13,
  "editor.cursorBlinking": "smooth",
  "editor.mouseWheelZoom": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.guides.bracketPairs": false,
  "editor.cursorSurroundingLines": 10,
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "trailing",
  "editor.smoothScrolling": true,
  "editor.wordWrap": "on",
  "editor.lineNumbers": "relative",
  "editor.stickyScroll.enabled": false,
  "editor.suggest.showStatusBar": true,
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.suggestSelection": "recentlyUsed",
  //---------- Debug ----------//
  "debug.toolBarLocation": "floating",
  //---------- Indent ----------//
  "indentRainbow.indicatorStyle": "light",
  "indentRainbow.lightIndicatorStyleLineWidth": 1,
  "indentRainbow.colors": [
    "rgba(255,255,64,0.3)",
    "rgba(127,255,127,0.3)",
    "rgba(255,127,255,0.3)",
    "rgba(79,236,236,0.3)"
  ],
  //---------- Git ----------//
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,
  "git.openRepositoryInParentFolders": "never",
  "git.blame.editorDecoration.enabled": false,
  "git.blame.statusBarItem.enabled": false,
  "gitlens.views.commitDetails.files.layout": "list",
  //---------- Clipboard-Manager ----------//
  "clipboard-manager.snippet.enabled": false,
  //---------- NoteBook ----------//
  "notebook.output.wordWrap": true,
  "notebook.lineNumbers": "on",
  "notebook.output.textLineLimit": 50,
  //---------- Databse Client ----------//
  "database-client.telemetry.usesOnlineServices": false,
  "database-client.autoSync": true,
  //---------- Flutter & Dart ----------//
  "dart.lineLength": 200,
  "dart.warnWhenEditingFilesOutsideWorkspace": false,
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": true,
  "dart.openDevTools": "flutter",
  "dart.flutterCreateOrganization": null,
  "dart.devToolsLocation": {
    "default": "active"
  },
  //---------- Python ----------//
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "python.languageServer": "Default",
  "python.analysis.typeCheckingMode": "standard",
  "python.analysis.completeFunctionParens": true,
  "python.analysis.inlayHints.callArgumentNames": "off",
  "python.analysis.inlayHints.pytestParameters": false,
  "python.analysis.inlayHints.functionReturnTypes": false,
  "python.analysis.inlayHints.variableTypes": false,
  //---------- Golang ----------//
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "never" // 关键设置
    },
    "editor.formatOnSave": false // 双重保障
  },
  "go.toolsManagement.autoUpdate": false, // 禁止自动更新工具
  //---------- Swift ----------//
  "[swift]": {
    "editor.defaultFormatter": "vknabel.vscode-swiftformat",
    "editor.inlayHints.enabled": "offUnlessPressed"
  },
  "swiftlint.enable": false,
  "swiftlint.onlyEnableWithConfig": true,
  "swiftlint.configSearchPaths": [
    "~/.config/vscode/language/swift/swiftlint.yml",
    ".swiftlint.yml"
  ],
  "swiftformat.configSearchPaths": [
    "~/.config/vscode/language/swift/swiftformat.config",
    ".swiftformat"
  ],
  "swift.sourcekit-lsp.backgroundIndexing": "auto",
  //---------- Objective-C ----------//
  "[objective-c]": {
    "editor.defaultFormatter": "nofoot.clang-format-support-style-variable",
    "editor.inlayHints.enabled": "offUnlessPressed"
  },
  //---------- HTML ----------//
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  //---------- CSS ----------//
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  //---------- JavaScript ----------//
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  //---------- TypeScript ----------//
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  //---------- JSON ----------//
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  //---------- YAML ----------//
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  //---------- Clang-Format ----------//
  "clang-format.language.objective-c.style": "file:${env.HOME}/.config/vscode/language/objective-c/clang-format.config",
  //---------- ESLint ----------//
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  //---------- LLDB ----------//
  "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/LLDB",
  //---------- LeetCode ----------//
  "leetcode.endpoint": "leetcode-cn",
  "leetcode.hint.commandShortcut": false,
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.workspaceFolder": "/Users/nofoot/.leetcode",
  //---------- Copilot ----------//
  "github.copilot.enable": {
    // 禁用 Copliot
    "*": false
  },
  //---------- Vim ----------//
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.searchHighlightColor": "rgba(150, 150, 150, 0.4)",
  "vim.useSystemClipboard": true,
  "vim.enableNeovim": false,
  "vim.smartRelativeLine": true,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {},
  "vim.insertModeKeyBindings": [],
  "vim.normalModeKeyBindingsNonRecursive": [
    // 光标移动
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<C-d>"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<C-u>"]
    },
    {
      "before": ["H"],
      "after": ["3", "h"]
    },
    {
      "before": ["L"],
      "after": ["3", "l"]
    },
    {
      "before": ["W"],
      "after": ["3", "w"]
    },
    {
      "before": ["E"],
      "after": ["3", "e"]
    },
    {
      "before": ["B"],
      "after": ["3", "b"]
    },
    {
      "before": ["<C-o>"],
      "commands": ["workbench.action.navigateBack"]
    },
    {
      "before": ["<C-i>"],
      "commands": ["workbench.action.navigateForward"]
    },
    // 窗口操作
    {
      "before": ["z", "h"],
      "commands": ["workbench.action.splitEditorLeft"]
    },
    {
      "before": ["z", "j"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["z", "k"],
      "commands": ["workbench.action.splitEditorUp"]
    },
    {
      "before": ["z", "l"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["leader", "k"],
      "commands": ["workbench.action.focusAboveGroup"]
    },
    {
      "before": ["leader", "j"],
      "commands": ["workbench.action.focusBelowGroup"]
    },
    {
      "before": ["leader", "h"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["leader", "l"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    {
      "before": ["<up>"],
      "commands": ["workbench.action.increaseViewSize"]
    },
    {
      "before": ["<down>"],
      "commands": ["workbench.action.decreaseViewSize"]
    },
    {
      "before": ["<left>"],
      "commands": ["workbench.action.decreaseViewWidth"]
    },
    {
      "before": ["<right>"],
      "commands": ["wrkbench.action.increaseViewWidth"]
    },
    {
      "before": ["q", "q"],
      "commands": ["workbench.action.closeOtherEditors"]
    },
    {
      "before": ["t", "t"],
      "commands": ["workbench.action.files.newUntitledFile"]
    },
    // 搜索
    {
      "before": ["leader", "f"],
      "commands": ["leap.find"]
    },
    {
      "before": ["leader", "e", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["leader", "<enter>"],
      "commands": [":nohl"]
    },
    // 代码格式化
    {
      "before": ["leader", "g", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    // 禁用
    {
      "before": ["g", "f"],
      "commands": [""]
    },
    // 文本编辑
    {
      "before": [";"],
      "after": [":"]
    },
    {
      "before": ["S"],
      "commands": [":w"]
    },
    {
      "before": ["Q"],
      "commands": [":q"]
    },
    {
      "before": ["`"],
      "after": ["~"]
    },
    {
      "before": ["'"],
      "after": ["%"]
    },
    {
      "before": [","],
      "after": ["0"]
    },
    {
      "before": ["."],
      "after": ["$"]
    },
    {
      "before": ["v", "'"],
      "after": ["v", "%"]
    },
    {
      "before": ["d", "'"],
      "after": ["d", "%"]
    },
    {
      "before": ["c", "'"],
      "after": ["c", "%"]
    },
    {
      "before": ["v", ","],
      "after": ["v", "0"]
    },
    {
      "before": ["d", ","],
      "after": ["d", "0"]
    },
    {
      "before": ["c", ","],
      "after": ["c", "0"]
    },
    {
      "before": ["v", "."],
      "after": ["v", "$", "h"]
    },
    {
      "before": ["d", "."],
      "after": ["v", "$", "h", "d"]
    },
    {
      "before": ["c", "."],
      "after": ["c", "$"]
    },
    {
      "before": ["Y"],
      "after": ["v", "$", "y"]
    },
    {
      "before": ["U"],
      "after": ["<C-r>"]
    },
    {
      "before": ["<enter>"],
      "commands": ["editor.action.smartSelect.expand"]
    },
    {
      "before": ["<backspace>"],
      "commands": ["editor.action.smartSelect.shrink"]
    },
    // 代码提示
    {
      "before": ["g", "j"],
      "commands": ["editor.action.revealDeclaration"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "o"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["g", "h"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["g", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["g", "r"],
      "commands": ["references-view.find"]
    },
    {
      "before": ["g", "R"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["g", "t"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["g", "T"],
      "commands": ["workbench.actions.view.problems"]
    },
    // 文件资源管理器
    {
      "before": ["leader", "w"],
      "commands": ["workbench.view.explorer"]
    },
    //---------- 引用 ----------//
    {
      "before": ["leader", "r"],
      "commands": ["workbench.view.extension.references-view"]
    },
    //---------- 书签 ----------//
    {
      "before": ["leader", "m"],
      "commands": ["bookmarksExplorer.focus"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    // 光标移动
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<C-d>"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<C-u>"]
    },
    {
      "before": ["H"],
      "after": ["3", "h"]
    },
    {
      "before": ["L"],
      "after": ["3", "l"]
    },
    {
      "before": ["W"],
      "after": ["3", "w"]
    },
    {
      "before": ["E"],
      "after": ["3", "e"]
    },
    {
      "before": ["B"],
      "after": ["3", "b"]
    },
    // 代码格式化
    {
      "before": ["leader", "g", "f"],
      "commands": ["editor.action.formatSelection"]
    },
    // 禁用
    {
      "before": ["g", "f"],
      "commands": [""]
    },
    //文本编辑
    {
      "before": ["<enter>"],
      "commands": ["editor.action.smartSelect.expand"]
    },
    {
      "before": ["<backspace>"],
      "commands": ["editor.action.smartSelect.shrink"]
    }
  ]
  //---------- End ----------//
}
